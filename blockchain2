import json
import hashlib

def gcd(a, b):
    while b:
        a, b = b, a % b 
    return a

def mod_inverse(e, phi):
    for d in range(1, phi):
        if (e * d) % phi == 1:
            return d
    return None
def sha256(data):
    return hashlib.sha256(data.encode()).hexdigest()


def generate_keys():
  
    p = 349
    q = 397
    n = p * q
    phi = (p - 1) * (q - 1)
    e = 17  
    d = mod_inverse(e, phi)
    return (e, n), (d, n)
